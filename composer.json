{
  "name": "execut/yii2-books-seo",
  "description": "Yii2 execut/yii2-seo and execut/yii2-books integration package",
  "keywords": ["yii2 seo", "yii2 books", "yii2 books seo integration"],
  "homepage": "https://github.com/execut/yii2-books-seo",
  "type": "yii2-extension",
  "license": "Apache-2.0",
  "support": {
    "issues": "https://github.com/execut/yii2-books-seo/issues?state=open",
    "wiki": "https://github.com/execut/yii2-books-seo/wiki",
    "source": "https://github.com/execut/yii2-books-seo"
  },
  "authors": [
    {
      "name": "Mamaev Yuriy (eXeCUT)"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "yiisoft/yii2": "@dev",
    "execut/yii2-books": "*",
    "execut/yii2-seo": "*"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.0",
    "codeception/module-yii2": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/verify": "~0.5.0 || ~1.1.0"
  },
  "config": {
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "execut\\booksSeo\\": "src"
    }
  }
}